<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.kaustubh.ecommerce">     <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <uses-permission android:name="android.permission.INTERNET" />    <application        android:allowBackup="true"        android:icon="@drawable/e_smart"        android:label="@string/app_name"        android:roundIcon="@drawable/e_smart"        android:supportsRtl="true"        android:theme="@style/AppTheme"        android:usesCleartextTraffic="true">        <activity android:name=".AcceptTicket"></activity>        <activity android:name=".MyBooking" />        <activity android:name=".TicketDetail" />        <activity            android:name=".VendorHome"            android:theme="@style/AppTheme.NoActionBar" />        <activity android:name=".Feedback" />        <activity android:name=".SubCategory" />        <activity android:name=".Enquiry" />        <activity            android:name=".Drower"            android:label="@string/title_activity_drower"            android:theme="@style/AppTheme.NoActionBar" />        <activity android:name=".SignIn" />        <activity android:name=".MainActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>    </application></manifest>